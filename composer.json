{
	"name": "flarum-lang/turkmen",
	"type": "flarum-extension",
	"description": "Turkmen language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/turkmen",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"tk-locale",
		"turkmen-locale",
		"turkmen"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/turkmen/issues",
		"source": "https://github.com/flarum-lang/turkmen"
	},
	"require": {
		"flarum/core": "^1.7"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Turkmen",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "tk",
			"title": "Turkmen"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
